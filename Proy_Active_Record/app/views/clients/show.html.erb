<div class="container" align="center" style="width: 500px">
<div class="panel panel-default" align="left" >
<div class="panel-heading" bgcolor="#f5f5f" align="center"><label class="titulo">Detail of Client <%=@client.first_name%> <%= @client.last_name%></label></div>
<p><strong>Client id: </strong> $<%= @client.client_id %><br />
<strong>Identity Doc :</strong> <%= @client.identity_doc %><br />
<strong>First Name :</strong> <%= @client.first_name %><br />
<strong>Last Name :</strong> <%= @client.last_name %><br />
<strong>Last Age :</strong> <%= @client.age %><br />
<strong>Created Date:</strong> <%= @client.created_at %><br />
</p>
<hr />
<p align="center">
<%= link_to 'Back', {:action => 'list'},class:"btn btn-primary"%>
</p>
</div>
</div>
