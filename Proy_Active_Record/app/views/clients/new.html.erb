<div class="container" align="center" style="width: 500px">
<div class="panel panel-default" align="left" >
	<div class="panel-heading" bgcolor="#f5f5f"><label class="titulo">Create New Client</label></div>
	<div class="panel-body">
	<br>
<%= form_for @client,url: {action: "create"} do |f|%>
<p><label>Client Id.</label>:
<%= f.text_field :client_id %></p>
<p><label>Identity doc.</label>:
<%= f.text_field :identity_doc %></p>
<p><label>First Name</label>:
<%= f.text_field :first_name %></p>
<p><label>Last Name</label>:
<%= f.text_field :last_name %></p>
<p><label>Age</label><br/>
<%= f.text_field:age %></p>
<%= f.submit class:"btn btn-primary"%>
<% end %>

</div>
</div>
<p align="center"></p><%= link_to 'Back', {:action => 'list' },class:"btn btn-primary" %></p>
</div>
